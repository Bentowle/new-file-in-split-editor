{
  "name": "new-file-in-split-editor",
  "displayName": "New File in Split Editor",
  "description": "Description of your extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openSplitWindow"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.openSplitWindow",
        "title": "Open New File in Split Editor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "eslint": "^7.10.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bentowle/new-file-in-split-editor.git"
  },
  "publisher": "lowend"
}